.fc-event-hori .ui-resizable-w {
  top: 0 !important;
  left: -3px !important;
  width: 7px !important;
  height: 100% !important;
  cursor: w-resize
}
.fc-event-hori .ui-resizable-handle {
  _padding-bottom: 14px
